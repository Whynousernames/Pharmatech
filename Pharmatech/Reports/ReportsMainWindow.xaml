                    <Button.Background>
                        <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                            <GradientStop Color="#FF2F98BF"/>
                            <GradientStop Color="#FF424EC3" Offset="1"/>
                        </LinearGradientBrush>
                    </Button.Background>
                </Button>
                <Button x:Name="button_filterByMedicationName" Content="Medication Name" HorizontalAlignment="Left" Height="56" Margin="251,159,0,0"  VerticalAlignment="Top" HorizontalContentAlignment="Center" Width="207"  BorderBrush="{x:Null}" Foreground="White" FontSize="24" Click="button_filterByMedicationName_Click" >
